{
  "name": "personal-website",
  "version": "1.0.0",
  "description": "Personal website of Johan Baaij",
  "author": "Johan Baaij",
  "private": true,
  "scripts": {
    "git-env-sha": "NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD`",
    "dev": "yarn run git-env-sha nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "yarn run git-env-sha nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "yarn lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.5",
    "@nuxtjs/axios": "^5.13.3",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/markdownit": "^1.2.9",
    "highlight.js": "^10.0.0",
    "mdi-vue": "^1.4.7",
    "nuxt": "^2.0.0",
    "nuxt-webfontloader": "^1.1.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.5",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.2",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@nuxtjs/tailwindcss": "^1.4.1",
    "@types/highlight.js": "^9.12.3",
    "@types/webpack-env": "^1.15.2",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^25.4.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.0.0",
    "jest": "^26.4.0",
    "lint-staged": "^10.1.7",
    "nuxt-property-decorator": "^2.7.2",
    "postcss-nesting": "^7.0.1",
    "prettier": "^2.0.5",
    "stylelint": "^13.3.3",
    "vue-jest": "^4.0.0-0"
  }
}
